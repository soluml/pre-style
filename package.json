{
  "name": "pre-style",
  "version": "2.0.3-alpha",
  "description": "",
  "main": "./dist/index.js",
  "bin": {
    "pre-style": "./dist/bin/index.js"
  },
  "files": [
    "README.md",
    "dist/**/*"
  ],
  "exports": {
    "./babel": {
      "default": "./dist/@babel/index.js"
    },
    "./@babel": {
      "default": "./dist/@babel/index.js"
    },
    "./postcss": {
      "default": "./dist/postcss/index.js"
    },
    "./postcss/getJSON": {
      "default": "./dist/postcss/saveJSONToPreStyleCache.js"
    }
  },
  "scripts": {
    "build": "tsc --outDir dist",
    "lint": "eslint src/*.ts bin/*.ts test/**/*.js",
    "pretest": "npm run lint && npm run build",
    "test:src": "jest src",
    "test:bin": "jest bin",
    "test:@babel": "jest @babel --no-cache",
    "test:postcss": "jest postcss",
    "test": "npm run test:src && npm run test:bin && npm run test:@babel && npm run test:postcss",
    "prestart": "npm run build",
    "start": "node dist/bin www/404.html -d docs"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/soluml/pre-style/"
  },
  "bugs": {
    "url": "https://github.com/soluml/pre-style/issues"
  },
  "homepage": "https://pre.style",
  "author": {
    "name": "Benjamin Solum",
    "email": "benjamin@soluml.com",
    "url": "http://soluml.com"
  },
  "license": "MIT",
  "engines": {
    "node": ">=12.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@babel/traverse": "^7.15.4",
    "@babel/types": "^7.15.6",
    "@types/css-tree": "^1.0.6",
    "@types/csso": "^4.2.0",
    "@types/find-cache-dir": "^3.2.1",
    "@types/ndjson": "^2.0.1",
    "@types/node": "^16.9.1",
    "@typescript-eslint/eslint-plugin": "^4.31.1",
    "@typescript-eslint/parser": "^4.31.1",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jest": "^24.4.2",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.2.0",
    "prettier": "^2.4.1",
    "typescript": "^4.4.3"
  },
  "dependencies": {
    "at-rule-packer": "^0.3.0",
    "chalk": "^4.1.2",
    "commander": "^8.2.0",
    "css-tree": "^1.1.3",
    "csso": "^4.2.0",
    "fast-glob": "^3.2.7",
    "find-cache-dir": "^3.3.2",
    "ndjson": "^2.0.0",
    "postcss": "^8.3.11",
    "postcss-selector-parser": "^6.0.6",
    "sass": "^1.39.2",
    "sweatmap": "^0.3.5"
  },
  "jest": {
    "rootDir": ".",
    "testMatch": [
      "**/test/**/*.js"
    ]
  }
}
