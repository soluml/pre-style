{
  "name": "Pre-Style-Site",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "style": "prestyle html -d '../../tmp' -o '../src/www/css/_prestyle.scss' -p 'css/_vars.scss'",
    "sass": "node-sass css --output '../../docs' --output-style compressed",
    "html": "html-minifier --input-dir '../../tmp/html' --output-dir '../../docs' --collapse-boolean-attributes --collapse-whitespace --html5",
    "build": "npm run style && npm run sass && npm run html",
    "start": "npm run build"
  },
  "devDependencies": {
    "autoprefixer": "6.5.0",
    "cssnano": "3.7.5",
    "html-minifier": "^3.3.1",
    "node-sass": "^4.5.0",
    "pre-style": "^1.0.7-alpha"
  }
}
